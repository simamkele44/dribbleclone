,\n  Route,\n  Link, Redirect\n} from \"react-router-dom\";\n\nfunction App() {\n  const [position, setPosition] = useState(0);\n\n  return (\n    <div className=\"App\">\n      <Router>\n        <Switch>\n          <Route exact path=\"/\">\n            <Redirect to=\"/shots/popular\"/>\n          </Route>\n          <PositionContext.Provider value={{position, setPosition}}>\n          <Route path=\"/shots/popular\" component={Home} />\n          <Route path=\"/shots/animation\" component={Animation} />\n        \n          <Route path=\"/shots/branding\">\n            <Branding/>\n          </Route>\n          <Route path=\"/shots/illustration\">\n            <Illustration/>\n          </Route>\n          <Route path=\"/shots/mobile\">\n            <Mobile/>\n          </Route>\n          <Route path=\"/shots/productdesign\">\n            <Productdesign/>\n          </Route>\n          <Route path=\"/shots/print\">\n            <Print/>\n          </Route>\n          <Route path=\"/shots/typography\">\n            <Typography/>\n          </Route>\n          <Route path=\"/shots/webdesign\">\n            <Webdesign/>\n          </Route>\n\n          <Route path=\"/detail/:id\">\n            <Detail/>\n          </Route>\n        </PositionContext.Provider>\n        </Switch>\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n","import { createSlice } from \"@reduxjs/toolkit\";\n\nconst initialState = {\n    position: 1\n}\n\nconst positionSlice = createSlice({\n    name: \"position\",\n    initialState,\n    reducers: {\n        setPosition: (state, action) => {\n            state.position = action.payload;\n        }\n    }\n})\n\nexport const { setPosition } = positionSlice.actions;\n\nexport const selectPosition = (state) => state.position.position;\n\nexport default positionSlice.reducer;","import { configureStore } from '@reduxjs/toolkit';\nimport positionSlice from '../features/position/positionSlice';\nimport positionReducer from '../features/posi